      "cell_type": "code",
      "metadata": {
        "id": "-TR0BcxPVR3l"
      },
      "source": [
        "import turtle\n",
        "t = turtle.Turtle()\n",
        "t.shape(\"turtle\")\n",
        "\n",
        "x1 = int(input(\"x1: \"))\n",
        "y1 = int(input(\"y1: \"))\n",
        "x2 = int(input(\"x2: \"))\n",
        "y2 = int(input(\"y2: \"))\n",
        "dist = ((x1-x2)** 2 + (y1-y2)**2)**0.5\n",
        "\n",
        "t.up()\n",
        "t.goto()\n",
        "t.down()\n",
        "t.goto(x2,y2)\n",
        "\n",
        "t.write(\"점의 길이=\"+str(dist))\n",
        "t._screen.exitonclick()  # 화면을 마우스로 클릭해야 종료되게 하는 부분"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Tk3ZO5aWYoxi"
      },
      "source": [
        "from turtle import*\n",
        "shape(\"turtle\")\n",
        "\n",
        "x1 =int(input(\"x1: \")); y1 = int(input(\"y1: \"))\n",
        "x2 = int(input(\"x2: \")); y2 = int(input(\"y2: \"))\n",
        "dist = ((x1-x2)** 2 + (y1-y2)**2)**0.5\n",
        "\n",
        "up(); goto(x1,y1); down(); goto(x2,y2)\n",
        "\n",
        "write(\"점의 길이=\"+str(dist))\n",
        "exitonclick()  # 화면을 마우스로 클릭해야 종료되게 하는 부분"
      ],
      "execution_count": null,
      "outputs": []
