        "from math import*\n",
        "x1 = int(input(\"x1: \"))\n",
        "y1 = int(input(\"y1: \"))\n",
        "x2 = int(input(\"x2: \"))\n",
        "y2 = int(input(\"y2: \"))\n",
        "dist = math.sqrt((x2-x1)**2 + (y2-y1)**2 )\n",
        "print(\"두 점 사이의 거리=\", dist)\n",
        "\n",
        "from turtle import*\n",
        "shape(\"turtle\")\n",
        "\n",
        "left(45)\n",
        "forward(141)\n",
        "\n",
        "setheading(0)\n",
        "goto(0,0)\n",
        "forward(100)\n",
        "left(90)\n",
        "forward(100)\n",
        "write(\"닫으려면 화면 클릭\")\n",
        "exitonclick()  # 화면을 마우스로 클릭해야 종료되게 하는 부분"
